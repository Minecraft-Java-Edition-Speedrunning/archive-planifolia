{
  "schemaVersion": 1,
  "id": "planifolia",
  "version": "${version}",
  "name": "Planifolia",
  "description": "Vanilla VideoOptionsScreen for sodium",
  "authors": [
    "tildejustin"
  ],
  "contact": {
    "homepage": "https://github.com/tildejustin/planifolia",
    "sources": "https://github.com/tildejustin/planifolia",
    "issues": "https://github.com/tildejustin/planifolia/issues"
  },
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "mixinsquared": [
      "dev.tildejustin.planifolia.PlanifoliaCanceller"
    ]
  },
  "mixins": [
    "planifolia.mixins.json"
  ],
  "suggests": {
    "sodium": ">=0.6"
  },
  "accessWidener": "planifolia.accesswidener",
  "custom": {
    "sodium:options": {
      "mixin.features.gui.hooks.settings": false
    }
  }
}
